<div class="m-3">
  <h1><%= @list.name %></h1>
</div>

<div>
  <ul>
    <% @list.movies.each do |movie|%>
    <li><%= movie.title %></li>
    <% end %>
  </ul>
</div>

<div>
  <%= simple_form_for [@list, @bookmark] do |f| %>
    <%= f.input :comment %>
    <%= f.association :movie %>
    <%# <%= f.association :list %>
    <%= f.submit :submit, class: "btn btn primary" %>
  <% end %>
</div>
